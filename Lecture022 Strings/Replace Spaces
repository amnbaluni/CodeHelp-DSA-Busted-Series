#include <bits/stdc++.h> 
#include <string>
string replaceSpaces(string &str){
	string temp="";
    for(int i=0;i<str.length();i++)
    {
        if(str[i]==' ')
           {
               temp.push_back('@');
               temp.push_back('4');
               temp.push_back('0');
           }
        else
            temp.puch_back(str[i]);
    }
    return temp;
}
